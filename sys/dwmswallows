# _NET_CLIENT_LIST(WINDOW): window id # 0x6400617, 0x6200006, 0x4a00006, 0x1400006, 0x4200006, 0x7e00002, 0x64000e0, 0x64002b4, 0x7c00002, 0x5400002, 0x5e00002,
# 0x2600006, 0x9400006, 0x8200006, 0x5000006, 0x2000006, 0x3200001, 0x6600006, 0x9600006, 0xcc00006, 0x2200006, 0x9800006, 0x4600006, 0x7600006

ACTIVE_WINDOW=$(xprop -root | grep '_NET_ACTIVE_WINDOW(WINDOW): window id #' | awk '{print $5}')
NEXT_WINDOW=$(xprop -root | grep '_NET_CLIENT_LIST(WINDOW): window id #' | awk '{print $5}')

dwmswallow $ACTIVE_WINDOW $NEXT_WINDOW


